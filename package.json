  },
  "dependencies": {
    "@generates/dotter": "^2.0.2",
    "@generates/merger": "^0.1.3",
    "@generates/swag": "^0.0.24",
    "@generates/swag-squad": "^0.0.27",
    "@headlessui/react": "^1.3.0",
    "@ianwalter/http": "^6.0.0",
    "@ianwalter/nrg": "^0.25.0",
    "@popperjs/core": "^2.9.2",
    "@stitches/react": "^0.2.3",
    "autoprefixer": "^10.3.1",
    "clsx": "^1.1.1",
    "common-tags": "^1.8.0",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "nanoid": "^3.1.23",
    "next": "^11.0.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.11.1",
