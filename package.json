    "eslint-config-next": "^11.0.1"
  },
  "nrg": {
    "app": "app/index.mjs"
  },
  "renovate": {
    "extends": [
      "@ianwalter"
    ],
    "packageRules": [
      {
        "packagePatterns": [
          "core-js"
        ],
        "schedule": [
          "on the last day of the month"
        ]
      }
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter",
      "next"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "react/jsx-no-target-blank": 0
    }
  },
  "eslintIgnore": [
    ".next"
  ]
}
