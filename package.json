{
  "private": true,
  "name": "jscommons",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port ${PORT:-11019}",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 --port ${PORT:-11019}",
    "lint": "eslint --ext js,mjs .",
    "test": "bff",
